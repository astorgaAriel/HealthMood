{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite",
  "public": true,
  "routes": [
    {
      "src": "/logo.png",
      "dest": "/logo.png",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
